            "key_code": "4",
            "modifiers": {
              "mandatory": [
                "right_command"
              ]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/chunkc tiling::window --send-to-desktop 4"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "5",
