            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "7",
            "modifiers": {
              "mandatory": [
                "right_command"
              ]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/chunkc tiling::window --send-to-desktop 7"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "8",
