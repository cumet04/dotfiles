{
  "title": "chunkwm key bindings",
  "rules": [
    {
      "description": "focus application",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "t",
            "modifiers": {
              "mandatory": [
                "left_control"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open '/Applications/iTerm.app'"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "b",
            "modifiers": {
              "mandatory": [
                "left_control"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open '/Applications/Google Chrome.app'"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "c",
            "modifiers": {
              "mandatory": [
                "left_control"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open '/Applications/Visual Studio Code.app'"
            }
          ]
        }
      ]
    },
    {
      "description": "move window",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "close_bracket",
            "modifiers": {
              "mandatory": [
                "right_command"
              ]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/chunkc tiling::window --swap next"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "open_bracket",
            "modifiers": {
              "mandatory": [
                "right_command"
              ]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/chunkc tiling::window --swap prev"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "1",
            "modifiers": {
              "mandatory": [
                "right_command"
              ]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/chunkc tiling::window --send-to-desktop 1"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "2",
            "modifiers": {
              "mandatory": [
                "right_command"
              ]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/chunkc tiling::window --send-to-desktop 2"
